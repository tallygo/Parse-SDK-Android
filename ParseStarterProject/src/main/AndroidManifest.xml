<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tallygo.starter" >

    <application
        android:name="com.tallygo.starter.StarterApplication"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="com.tallygo.SERVER_URL"
            android:value="@string/parse_server_url" />
        <meta-data
            android:name="com.tallygo.APPLICATION_ID"
            android:value="@string/parse_app_id" />
        <meta-data
            android:name="com.tallygo.CLIENT_KEY"
            android:value="@string/parse_client_key" />

        <activity
            android:name="com.tallygo.starter.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
